config 'global'
	option	anon_swap	'0'
	option	anon_mount	'0'
	option	auto_swap	'1'
	option	auto_mount	'1'
	option	delay_root	'5'
	option	check_fs	'0'

config 'mount'
	option	target	'/overlay'
	option	device	'/dev/mmcblk0p3'
	option	fstype	'ext4'
	option	options	'rw,noatime'
	option	enabled	'1'

config 'mount'
	option	target	'/data'
	option	device	'/dev/mmcblk0p4'
	option	fstype	'ext4'
	option	options	'rw,noatime'
	option	enabled	'1'
